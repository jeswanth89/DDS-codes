// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

struct Node{
    int data;
    Node * left;
    Node * right;
    Node(int val){
        data=val;
        left=nullptr;
        right=nullptr;
        
    }
};
void preorder(Node * root);
void inorder(Node*root);
void postorder(Node*root);
int main (){
    Node*root= new Node(10);
    root->left=new Node(11);
    root->right=new Node(12);
    root->left->left=new Node(13);
    root->left->left=new Node(14);
    root->right->left=new Node(15);
    root->right->right=new Node(16);
    cout<<"Preorder:";
    preorder(root);
    cout<<endl;
    cout<<"Inorder:";
    inorder(root);
    cout<<endl;
    cout<<"Postorder:";
    postorder(root);
    cout<<endl;
}
void preorder(Node * root){
    if(root){
        cout<<root->data<<" ";
        preorder(root->left);
        preorder(root->right);
    }}
    void inorder(Node*root){
        if(root){
            inorder(root->left);
            cout<<root->data<<" ";
            inorder(root->right);
        }}
        void postorder(Node*root){
            if(root){
                postorder(root->left);
                postorder(root->right);
                cout<<root->data<<" ";
            }}
            
